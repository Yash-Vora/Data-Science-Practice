  <DataSets>
    <DataSet Name="CountryAndTerritoryWiseSales">
      <Query>
        <DataSourceName>AdventureWork</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Country">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Territory">
            <Value>=Parameters!Territory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CountryRegionCode, Name, SUM(SalesYTD) AS Total_Sales
FROM     Sales.SalesTerritory
WHERE  (CountryRegionCode IN (@Country)) AND (Name IN (@Territory))
GROUP BY CountryRegionCode, Name</CommandText>
      </Query>
      <Fields>
        <Field Name="CountryRegionCode">
          <DataField>CountryRegionCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Sales">
          <DataField>Total_Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
